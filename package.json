{
  "name": "node_server",
  "version": "1.0.0",
  "description": "Starter node server",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no specified\" && exit 1",
    "start": "node server.js",
    "start:local": "set NODE_ENV=local&&npm start",
    "start:prod": "set NODE_ENV=production&&npm start",
    "build": "cross-env NODE_ENV=production pm2 start server.js --watch ",
    "build:max": "cross-env NODE_ENV=production pm2 start server.js -i 2 --watch"
